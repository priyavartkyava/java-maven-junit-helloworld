pipeline
{
    agent 'any'
    tools
    {
      maven 'm3'
    }
    stages
    {
        stage ('Compile')
        {
            steps 
            {
               sh 'mvn compile'
            }
        }
        stage('Package')
        {
            steps
            {
             sh  'mvn package'
            }
        }
       stage('archive')
        {
            steps
            {
                archiveArtifacts *.jar
            }
        }
    }    
}
