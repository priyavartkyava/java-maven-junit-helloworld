pipeline
{
    agent 'any'
    parameters
    {
        string (
          defaultValue: '*',
          name: 'BRANCH_NAME')
        booleanParam ( 
            defaultValue: false,
            description: '',
            name: 'FORCE_FULL_BUILD')
    }
    environment 
    {
       GIT_BRANCH = 'master'
    }
    tools
    {
      maven 'm3'
    }
    stages
    {
        stage ('Compile')
        {
            steps 
            {
               sh 'mvn compile'
            }
        }
        stage('Package')
        {
            steps
            {
             sh  'mvn package'
            }
        }
       stage('archive')
        {
            steps
            {
                archiveArtifacts 'target/*.jar'
            }
        }
    }    
}
