pipeline
{
    agent 'any'
    parameters
    {
        string (
          defaultValue: 'develop',
          name: 'BRANCH_NAME')
        string ( 
            defaultValue: 'DEV',
            description: 'Environment To release on',
            name: 'BUILD_ENV')
    }
    tools
    {
      maven 'm3'
    }
    stages
    {
        stage ('Compile')
        {
            steps 
            {
              ansiblePlaybook(credentialsId: 'devops', inventory: '34.218.51.237,' , playbook: 'scripts/ansible/play.yml', extras: '-e target_hosts="34.218.51.237"')
            }
        }
        stage('Package')
        {
            steps
            {
             sh  'mvn package'
            }
        }
       
       stage("foo") 
        {
            steps {
                sh 'printenv'
               
                
            }
        }
        
       stage('archive')
        {
            steps
            {
                archiveArtifacts 'target/*.jar'
            }
        }
    }    
}
